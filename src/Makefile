CXX		=	g++
CXXFLAGS	=	-g -O0 -Wall

all:		predict

predict:	predict.cc trace.cc predictor.h branch.h trace.h my_predictor.h
		$(CXX) $(CXXFLAGS) -o $@ predict.cc trace.cc

predict1:	predict.cc trace.cc predictor.h branch.h trace.h my_predictor.h
		$(CXX) $(CXXFLAGS) -o $@ predict.cc trace.cc

predict2:	predict.cc trace.cc predictor.h branch.h trace.h my_predictor.h
		$(CXX) $(CXXFLAGS) -o $@ predict.cc trace.cc

predict3:	predict.cc trace.cc predictor.h branch.h trace.h my_predictor.h
		$(CXX) $(CXXFLAGS) -o $@ predict.cc trace.cc

predict4:	predict.cc trace.cc predictor.h branch.h trace.h my_predictor.h
		$(CXX) $(CXXFLAGS) -o $@ predict.cc trace.cc

predict5:	predict.cc trace.cc predictor.h branch.h trace.h my_predictor.h
		$(CXX) $(CXXFLAGS) -o $@ predict.cc trace.cc

predict6:	predict.cc trace.cc predictor.h branch.h trace.h my_predictor.h
		$(CXX) $(CXXFLAGS) -o $@ predict.cc trace.cc
clean:
		rm -f predict
